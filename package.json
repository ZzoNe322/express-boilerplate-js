{
  "name": "express-boilerplate-js",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "node ./scripts/start.js",
    "test": "jest",
    "test:js": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\""
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-plugin-jest": "^22.2.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.3"
  }
}
